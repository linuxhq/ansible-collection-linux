[rpmfusion-nonfree-updates]
name=RPM Fusion for EL {{ rpmfusion_releasever }} - Nonfree - Updates
#baseurl=http://download1.rpmfusion.org/nonfree/el/updates/{{ rpmfusion_releasever }}/$basearch/
{% if rpmfusion_releasever | int >= 9%}
metalink=http://mirrors.rpmfusion.org/metalink?repo=nonfree-el-updates-released-{{ rpmfusion_releasever }}&arch=$basearch
{% else %}
mirrorlist=http://mirrors.rpmfusion.org/mirrorlist?repo=nonfree-el-updates-released-{{ rpmfusion_releasever }}&arch=$basearch
{% endif %}
enabled={{ '1' if rpmfusion_repository_nonfree_updates | bool else '0' }}
{% if rpmfusion_releasever | int > 7 %}
type=rpm-md
{% endif %}
gpgcheck=1
repo_gpgcheck=0
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-rpmfusion-nonfree-el-{{ rpmfusion_releasever }}

